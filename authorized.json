uc77c\uc790}\n\uc608\uc57d\uac74\uc218: #{\uc608\uc57d\uac74\uc218}\n\n\uc608\uc57d\ud558\uc2e0 \uba54\uc2dc\uc9c0\ub294 \ubc1c\uc1a1 \uc608\uc815 \uc2dc\uac01\uc5d0 \uc21c\ucc28\uc801\uc73c\ub85c \ucc98\ub9ac\ub429\ub2c8\ub2e4. \uac10\uc0ac\ud569\ub2c8\ub2e4."}, {"order": "21", "template_code": "styleh_editors_survey", "template_name": "\uc2a4\ud0c0\uc77ch\ud3b8\uc9d1\uc704\uc6d0\uc124\ubb38", "template": "\uc548\ub155\ud558\uc2ed\ub2c8\uae4c, #{\uace0\uac1d\uc774\ub984}\ub2d8\n \n<\uc2a4\ud0c0\uc77c H>\ub97c \uad6c\ub3c5\ud574\uc8fc\uc154\uc11c \uac10\uc0ac\ud569\ub2c8\ub2e4.\n<\uc2a4\ud0c0\uc77c H>\uac00 \uc0c8 \uc637\uc744 \uc785\uc740\uc9c0\ub3c4 \uc5b4\ub290\ub367 6\uac1c\uc6d4\uc774 \ub418\uc5b4\uac11\ub2c8\ub2e4.\n\uc774\uc5d0 \uac1c\ud3b8\ub41c \ub9e4\uccb4\uc758 \ub9cc\uc871\ub3c4 \uc870\uc0ac\ub97c \uc704\ud574 9\uae30\uc640 10\uae30 \ud3b8\uc9d1\uc704\uc6d0\uc73c\ub85c \ud65c\ub3d9\ud558\uc5ec \uc8fc\uc168\ub358 \ubd84\ub4e4\uc744 \ub300\uc0c1\uc73c\ub85c \uc124\ubb38\uc870\uc0ac\ub97c \uc2e4\uc2dc\ud558\uac8c \ub418\uc5c8\uc2b5\ub2c8\ub2e4.\n\n\uc124\ubb38\uc5d0 \uc18c\uc694\ub418\ub294 \uc2dc\uac04\uc740 \uc57d #{\uc18c\uc694\uc2dc\uac04}\ubd84 \uc815\ub3c4 \uc18c\uc694\ub418\uba70, \uc544\ub798 [\ucc38\uc5ec\ud558\uae30] \ubc84\ud2bc\uc744 \ub204\ub974\uba74 \uce74\uce74\uc624\ud1a1 \ucc44\ud305\ucc3d \ub0b4\uc5d0\uc11c \uc124\ubb38\uc5d0 \uc751\ub2f5\ud558\uc2e4 \uc218 \uc788\uc2b5\ub2c8\ub2e4."}, {"order": "22", "template_code": "imtest_01", "template_name": "\ub098\ub294\ud14c\uc2a4\ud2b8_01", "template": "\uc548\ub155\ud558\uc138\uc694, #{\uc774\ub984}\ub2d8\n\ud3ec\ucf13\uc11c\ubca0\uc774\ub97c \uc774\uc6a9\ud574\uc8fc\uc154\uc11c \uac10\uc0ac\ud569\ub2c8\ub2e4. (\ud558\ud558)\n\n\ud3ec\ucf13\uc11c\ubca0\uc774\uc758 \uc774\uc6a9\uacfc\uc815\uc758 \ud3b8\uc758\uc131 \ud5a5\uc0c1\uc744 \uc704\ud574 '\ud3ec\ucf13\uc11c\ubca0\uc774 \ub9cc\uc871\ub3c4\uc870\uc0ac'\ub97c \uc9c4\ud589\ud558\uace0 \uc788\uc2b5\ub2c8\ub2e4. \uace0\uac1d\ub2d8\uc758 \uc18c